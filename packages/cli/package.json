{
  "name": "spheron-cli",
  "version": "1.0.1",
  "description": "CLI for working on projects and deploying them via Spheron ",
  "keywords": [
    "CLI",
    "Typescript",
    "Badass",
    "IPFS",
    "Filecoin",
    "Arweave",
    "Deploy",
    "Publish"
  ],
  "author": "Spheron Team",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc && cp src/scripts/clone.sh dist/scripts/clone.sh",
    "postinstall": "node ./dist/scripts/postinstall.js",
    "prettier": "prettier \"**/*.{css,scss,json,md,html,yml}\" --ignore-path=.eslintignore",
    "prettier:write": "prettier --write '**/*.{ts,tsx,json,md,hbs}' --ignore-path .gitignore",
    "prettier:check": "prettier --check '**/*.{ts,tsx,json,md,hbs}' --ignore-path .gitignore",
    "lint": "eslint . --ext .ts"
  },
  "bin": {
    "spheron": "./dist/index.js"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@spheron/storage": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "axios": "^1.3.2",
    "cli-progress": "^3.12.0",
    "eslint": "^8.33.0",
    "fs": "^0.0.1-security",
    "http": "^0.0.1-security",
    "inquirer": "^8.2.5",
    "nanospinner": "^1.1.0",
    "open": "^8.4.1",
    "path": "^0.12.7",
    "prettier": "^2.8.3",
    "prompts": "^2.4.2",
    "uuid": "^9.0.0",
    "yaml": "^2.2.1",
    "yargs": "^17.6.2"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.0",
    "@types/node": "^18.13.0",
    "@types/uuid": "^9.0.0",
    "@types/yargs": "17.0.22",
    "typescript": "^4.9.5"
  },
  "files": [
    "dist/"
  ]
}