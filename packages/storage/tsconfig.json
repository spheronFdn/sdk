{
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "strict": true,
      "skipLibCheck": true,
      "removeComments": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "forceConsistentCasingInFileNames": true,
      "rootDir": "./src",
      "outDir": "./dist/",
    },
    "include": ["./src/**/*"],
    "exclude": ["node_modules"]
  }
  